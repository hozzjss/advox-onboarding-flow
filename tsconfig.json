{
    "compilerOptions": {
        "paths": {
            "*": ["./*.d.ts"]
        },
        "downlevelIteration": true,
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": false
    }
}
